{
  "kind": "collectionType",
  "collectionName": "beritas",
  "info": {
    "singularName": "berita",
    "pluralName": "beritas",
    "displayName": "Berita"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "foto_header": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "tanggal_dibuat": {
      "type": "date",
      "required": true
    },
    "judul": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "deskripsi": {
      "type": "text",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "beritas"
    },
    "label_berita": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::label-berita.label-berita",
      "inversedBy": "beritas"
    },
    "isi_berita": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "cta_url": {
      "type": "string"
    },
    "prioritas": {
      "type": "boolean"
    },
    "priority_expiration": {
      "type": "datetime",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "prioritas"
            },
            true
          ]
        }
      }
    },
    "cta_display": {
      "type": "string"
    }
  }
}
